{
    "name": "doublethreedigital/yumyum",
    "autoload": {
        "psr-4": {
            "DoubleThreeDigital\\YumYum\\": "src"
        }
    },
    "extra": {
        "statamic": {
            "name": "YumYum",
            "description": "Transform data from RSS, JSON & CSV's into Statamic entries."
        },
        "laravel": {
            "providers": [
                "DoubleThreeDigital\\YumYum\\ServiceProvider"
            ]
        }
    },
    "require": {
        "statamic/cms": "^3.0 || ^3.1"
    },
    "require-dev": {
        "nunomaduro/collision": "^4.2",
        "orchestra/testbench": "^5.0"
    },
    "scripts": {
        "lint": [
            "php-cs-fixer fix ./src"
        ],
        "test": [
            "./vendor/bin/phpunit"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
